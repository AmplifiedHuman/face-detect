{
  "name": "face-detect",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.2",
    "clarifai": "^2.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-particles-js": "^3.3.0",
    "react-scripts": "^3.4.3",
    "react-tilt": "^0.1.4",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "env NODE_ENV=production npm run build:css && react-scripts build",
    "build:css": "npx tailwindcss build src/assets/tailwind.css -o src/assets/style.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/style.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.7.3"
  }
}
